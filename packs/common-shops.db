n for such a weapon. It lasts for 10 minutes.</p>"},"source":{"value":"Pathfinder Lost Omens: Pathfinder Society Guide"},"traits":{"value":["alchemical","consumable","precious"],"rarity":{"value":"uncommon"},"custom":""},"rules":[],"slug":"cold-iron-blanch-moderate","schema":{"version":0.709,"lastMigration":{"datetime":"2022-01-05T13:00:04.286+01:00","version":{"schema":0.708,"foundry":"9.238","system":"3.1.3.9639"}}},"quantity":{"value":4},"baseItem":null,"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"L"},"equippedBulk":{"value":""},"price":{"value":"140 gp"},"equipped":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"size":{"value":"med"},"identification":{"status":"identified","unidentified":{"name":"","img":"","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null,"autoUse":true,"autoDestroy":true},"spell":{"data":null,"heightenedLevel":null},"level":{"value":9},"consumableType":{"value":"potion"},"charges":{"value":"1","max":"1","_deprecated":true},"consume":{"value":null,"_deprecated":true},"autoUse":{"value":true,"_deprecated":true},"autoDestroy":{"value":true,"_deprecated":true}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"sarkMXaT0ELIQ2fE":3},"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.jnCP2zYJamUeZsJC"}}},{"_id":"ZeuyoypTDzkZSFRS","name":"Elixir of Life (Moderate)","type":"consumable","img":"systems/pf2e/icons/equipment/alchemical-items/alchemical-elixirs/elixir-of-life.webp","data":{"description":{"value":"<p><strong>Activate</strong> <span cl