lse},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","criticalConditionType":"","criticalConditionValue":null,"strikeConditionType":"","strikeConditionValue":null}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"sarkMXaT0ELIQ2fE":3},"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.n5yTlTs63o53n3k6"}}},{"_id":"16go0fk5t1W92iRV","name":"Gloaming Shard","type":"weapon","img":"systems/pf2e/icons/equipment/weapons/specific-magic-weapons/gloaming-shard.webp","data":{"description":{"value":"<p>The blade of this <em>+2 striking returning dagger</em> shines the color of twilight, with a triangular lattice design on the hilt. A thin string of darkness connects your shadow to that of the blade, even once it leaves your hand.</p>\n<hr />\n<p><strong>Activate</strong> <span class=\"pf2-icon\">2</span> Interact (shadow, teleportation)</p>\n<p><strong>Frequency</strong> once per hour</p>\n<hr />\n<p><strong>Effect</strong> You make a thrown Strike with the gloaming shard at a target within 60 feet. Whether you hit or miss, rather than returning to you, the blade pulls you down into your own shadow and along the connecting thread. You instantly teleport to a space adjacent to the target of your Strike, as <em>@Compendium[pf2e.spells-srd.VlNcjmYyu95vOUe8]{Dimension Door}</em>, and then catch the dagger in your hand.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["agile","conjuration","finesse","magical","shadow","thrown-10","versatile-s"],"rarity":"common","custom":""},"rules":[],"slug":"gloaming-shard","schema":{"version":0.752,"lastMigration":null},"quantity":1,"baseItem":"dagger","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"L"},"equippedBulk":{"value":""},"price":{"value":{"gp":1400}},"equipped":{"carryTyp