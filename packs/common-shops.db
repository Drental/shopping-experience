amage":{"value":0},"range":null,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":3},"strikingRune":{"value":"greaterStriking"},"specific":{"value":false},"propertyRune1":{"value":"greaterShock"},"propertyRune2":{"value":"greaterFrost"},"propertyRune3":{"value":"greaterFlaming"},"propertyRune4":{"value":null},"property1":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","criticalConditionType":"","criticalConditionValue":null,"strikeConditionType":"","strikeConditionValue":null}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"sarkMXaT0ELIQ2fE":3},"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.v8C4hkqMNeUk60Db"}}},{"_id":"Xf6ybYpLlKyoJt51","name":"Large Frost Brand","type":"weapon","img":"systems/pf2e/icons/equipment/weapons/specific-magic-weapons/frost-brand.webp","data":{"description":{"value":"<p>The blade of this +2 greater striking @Compendium[pf2e.equipment-srd.Sexud7FdxIrg50vU]{Frost (Greater)} greatsword appears to be nothing but icy shards. The frost brand automatically extinguishes non-magical fires in a 20-foot emanation. While wielding it, you gain fire resistance 5.</p>\n<p><strong>Activate</strong> <span class=\"pf2-icon\">D</span> command, Interact</p>\n<p><strong>Effect</strong> You swing the <em>frost brand</em> into the area of an ongoing magical fire, and the blade attempts to counteract the fire with a counteract modifier of +27. If it fails, it can't attempt to counteract the same fire again.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["cold","evocation","magical","versatile-p"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"frost-brand","schema":{"version":0.709,"lastMigration":null},"quantity":{"value":1},"baseItem":"greatsword","hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"2"},"equippedBulk":{"value":""},"price":{"value":"10,000 gp"},"equipped"